  <Dimension type="StandardDimension" visible="true" highCardinality="false" name="Lead Source">
    <Hierarchy visible="true" hasAll="true" allMemberName="All Lead Sources" primaryKey="LEAD_TK">
      <Table name="D_LEADS">
