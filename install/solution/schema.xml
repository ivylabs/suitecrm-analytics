    </Table>
    <DimensionUsage source="Products" name="Products" visible="true" foreignKey="PRODUCT_TK" highCardinality="false">
    </DimensionUsage>
    <DimensionUsage source="Accounts" name="Accounts" visible="true" foreignKey="BILLING_ACCOUNT_TK" highCardinality="false">
    </DimensionUsage>
    <Measure name="Total Sales Quantity" column="SALE_QUANTITY" aggregator="sum" visible="true">
