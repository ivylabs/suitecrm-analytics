      <Parameters>
         <Parameter default="20200101" name="dateFromKeyParam" type="String"/>
         <Parameter default="20200301" name="dateToKeyParam" type="String"/>
      </Parameters>
      <Query><![CDATA[SELECT
    SD.`YYYY-MM-DD`
