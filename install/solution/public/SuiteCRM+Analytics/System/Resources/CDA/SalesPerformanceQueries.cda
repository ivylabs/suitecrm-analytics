   <DataAccess access="public" connection="qTotalSalesProfitByDayQuery"
               id="qTotalSalesProfitByDayQuery"
               type="sql">
      <Name>qTotalSalesProfitByDayQuery</Name>
      <Cache duration="3600" enabled="true"/>
