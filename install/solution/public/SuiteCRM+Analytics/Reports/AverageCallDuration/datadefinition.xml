<?xml version="1.0" encoding="UTF-8"?>
<data-definition xmlns="http://reporting.pentaho.org/namespaces/engine/classic/bundle/data/1.0">
  <parameter-definition>
    <list-parameter name="pSelectStyle" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pStyles" key-column="Value" value-column="Name" default-value="SuiteCRMAnalytics">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Select Report Style</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pActivityFromDateFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pActivityDateOnlyFilterQuery" key-column="YYYY-MM" value-column="DATE_FILTER">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Call From Period</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pActivityToDateFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pActivityDateOnlyFilterQuery" key-column="YYYY-MM" value-column="DATE_FILTER">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Call To Period</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowChart" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pShowChartQuery" key-column="value" value-column="property" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Visualizations</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pSelectChartType" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pChartTypeQuery" key-column="ID" value-column="Value">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Chart Type</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowParameters" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pShowParameters" key-column="Value" value-column="property" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Parameters</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowDeletedRecords" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.Number" query="pShowDeletedRecords" key-column="Value" value-column="Name">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Deleted Records</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
  </parameter-definition>
  <data-source report-query="Report" limit="-1" timeout="0" ref="datasources/compound-ds.xml"/>
  <expression name="FUNC_PAGE_NUMBERS" class="org.pentaho.reporting.engine.classic.core.function.PageOfPagesFunction">
    <properties>
      <property name="format">page {0} of {1}</property>
      <property name="pageIncrement">1</property>
      <property name="startPage">1</property>
    </properties>
  </expression>
  <expression name="FUNC_TODAYS_DATE" formula="=TODAY()"/>
  <expression name="FUNC_PAGE_NUMBER" class="org.pentaho.reporting.engine.classic.core.function.PageFunction">
    <properties>
      <property name="pageIncrement">1</property>
      <property name="startPage">1</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_ROWS" class="org.pentaho.reporting.engine.classic.core.function.TotalItemCountFunction">
    <properties>
    </properties>
  </expression>
  <expression name="fChartParams" formula="=&quot;&amp;parampActivityFromDateFilter=&quot;&amp; URLENCODE([pActivityFromDateFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampActivityToDateFilter=&quot;&amp; URLENCODE([pActivityToDateFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampShowDeletedRecords=&quot;&amp; URLENCODE([pShowDeletedRecords];&quot;UTF8&quot;) &amp;&quot;&amp;outputType=svg&amp;_TRUST_USER_=&quot;&amp;[&quot;env::username&quot;]"/>
  <expression name="fChartURL" formula="=[&quot;env::pentahoBaseURL&quot;]&amp;&quot;plugin/cgg/api/services/draw?script=/public/SuiteCRM+Analytics/System/Resources/RCD/&quot; &amp; [fChartGeneratedURL] &amp; &quot;.js&amp;paramchart=AverageCallDuration&amp;paramchartType=ccc&quot; &amp; [pSelectChartType] &amp; &quot;Chart&quot; &amp; [fChartParams]"/>
  <expression name="fChartGeneratedURL" formula="=&quot;CGGChartGenerator&quot;"/>
  <expression name="fReportOutputType" formula="=IF(ISEXPORTTYPE(&quot;table/excel&quot;);&quot;This is Excel!&quot;;&quot;This is NOT Excel&quot;)"/>
  <expression name="fAverageByRow" formula="=([LEADS]+[CASES]+[OPPORTUNITIES])/3"/>
  <expression name="fAverageLeads" class="org.pentaho.reporting.engine.classic.core.function.ItemAvgFunction">
    <properties>
      <property name="field">LEADS</property>
      <property name="group">GROUPBY</property>
      <property name="roundingMode">4</property>
      <property name="scale">14</property>
    </properties>
  </expression>
  <expression name="fAverageCases" class="org.pentaho.reporting.engine.classic.core.function.ItemAvgFunction">
    <properties>
      <property name="field">CASES</property>
      <property name="group">GROUPBY</property>
      <property name="roundingMode">4</property>
      <property name="scale">14</property>
    </properties>
  </expression>
  <expression name="fAverageOpportunities" class="org.pentaho.reporting.engine.classic.core.function.ItemAvgFunction">
    <properties>
      <property name="field">OPPORTUNITIES</property>
      <property name="group">GROUPBY</property>
      <property name="roundingMode">4</property>
      <property name="scale">14</property>
    </properties>
  </expression>
  <expression name="fAverageAverageByRow" class="org.pentaho.reporting.engine.classic.core.function.ItemAvgFunction">
    <properties>
      <property name="field">fAverageByRow</property>
      <property name="group">GROUPBY</property>
      <property name="roundingMode">4</property>
      <property name="scale">14</property>
    </properties>
  </expression>
  <expression name="fFormattedLeads" formula="=INT([LEADS]/60)&amp;&quot;h &quot;&amp;INT(MOD([LEADS];60))&amp;&quot;m&quot;"/>
  <expression name="fFormattedCases" formula="=INT([CASES]/60)&amp;&quot;h &quot;&amp;INT(MOD([CASES];60))&amp;&quot;m&quot;"/>
  <expression name="fFormattedOpportunities" formula="=INT([OPPORTUNITIES]/60)&amp;&quot;h &quot;&amp;INT(MOD([OPPORTUNITIES];60))&amp;&quot;m&quot;"/>
  <expression name="fFormattedAverageByRow" formula="=INT([fAverageByRow]/60)&amp;&quot;h &quot;&amp;INT(MOD([fAverageByRow];60))&amp;&quot;m&quot;"/>
  <expression name="fFormattedAverageAverageByRow" formula="=INT([fAverageAverageByRow]/60)&amp;&quot;h &quot;&amp;INT(MOD([fAverageAverageByRow];60))&amp;&quot;m&quot;"/>
  <expression name="fFormattedAverageLeads" formula="=INT([fAverageLeads]/60)&amp;&quot;h &quot;&amp;INT(MOD([fAverageLeads];60))&amp;&quot;m&quot;"/>
  <expression name="fFormattedAverageCases" formula="=INT([fAverageCases]/60)&amp;&quot;h &quot;&amp;INT(MOD([fAverageCases];60))&amp;&quot;m&quot;"/>
  <expression name="fFormattedAverageOpportunities" formula="=INT([fAverageOpportunities]/60)&amp;&quot;h &quot;&amp;INT(MOD([fAverageOpportunities];60))&amp;&quot;m&quot;"/>
</data-definition>
