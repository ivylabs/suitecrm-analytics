<?xml version="1.0" encoding="UTF-8"?>
<data-definition xmlns="http://reporting.pentaho.org/namespaces/engine/classic/bundle/data/1.0">
  <parameter-definition>
    <list-parameter name="pSelectStyle" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pStyles" key-column="Value" value-column="Name" default-value="SuiteCRMAnalytics">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Select Report Style</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pOpportunityCreatedDateFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pOpportunityCreatedDateFilterQuery" key-column="YYYY-MM" value-column="DATE_FILTER">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Opportunity Created Period</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pOpportunityClosedDateFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pOpportunityClosedDateFilterQuery" key-column="YYYY-MM" value-column="DATE_FILTER">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Opportunity Closed Period</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">true</attribute>
    </list-parameter>
    <list-parameter name="pOpportunityLeadSourceFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pOpportunityLeadSourceFilterQuery" key-column="OPPORTUNITY_LEAD_SOURCE" value-column="OPPORTUNITY_LEAD_SOURCE">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Opportunity Lead Source</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pOpportunitySalesStageFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pOpportunitySalesStageFilterQuery" key-column="OPPORTUNITY_SALES_STAGE" value-column="OPPORTUNITY_SALES_STAGE">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Opportunity Sales Stage</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pOpportunityTypeFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pOpportunityTypeFilterQuery" key-column="OPPORTUNITY_TYPE" value-column="OPPORTUNITY_TYPE">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Opportunity Type</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowChart" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pShowChartQuery" key-column="value" value-column="property" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Visualizations</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pSelectChartType" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pChartTypeQuery" key-column="ID" value-column="Value">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Chart Type</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">true</attribute>
    </list-parameter>
    <list-parameter name="pShowParameters" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pShowParameters" key-column="Value" value-column="property" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Parameters</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowDeletedRecords" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.Number" query="ShowDeletedRecords" key-column="Value" value-column="Name" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Deleted Records</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
  </parameter-definition>
  <data-source report-query="Report" limit="-1" timeout="0" ref="datasources/compound-ds.xml"/>
  <expression name="FUNC_PAGE_NUMBERS" class="org.pentaho.reporting.engine.classic.core.function.PageOfPagesFunction">
    <properties>
      <property name="format">page {0} of {1}</property>
      <property name="pageIncrement">1</property>
      <property name="startPage">1</property>
    </properties>
  </expression>
  <expression name="FUNC_TODAYS_DATE" formula="=TODAY()"/>
  <expression name="fChartParams" formula="=&quot;&amp;parampOpportunityCreatedDateFilter=&quot;&amp; URLENCODE([pOpportunityCreatedDateFilter];&quot;UTF8&quot;) &amp; &quot;&amp;parampOpportunityClosedDateFilter=&quot;&amp; URLENCODE([pOpportunityClosedDateFilter];&quot;UTF8&quot;) &amp; &quot;&amp;parampOpportunityLeadSourceFilter=&quot;&amp; URLENCODE([pOpportunityLeadSourceFilter];&quot;UTF8&quot;) &amp; &quot;&amp;parampOpportunitySalesStageFilter=&quot;&amp; URLENCODE([pOpportunitySalesStageFilter];&quot;UTF8&quot;) &amp; &quot;&amp;parampOpportunityTypeFilter=&quot;&amp; URLENCODE([pOpportunityTypeFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampShowDeletedRecords=&quot;&amp; URLENCODE([pShowDeletedRecords];&quot;UTF8&quot;) &amp;&quot;&amp;outputType=svg&amp;_TRUST_USER_=&quot;&amp;[&quot;env::username&quot;]"/>
  <expression name="fChartURL" formula="=[&quot;env::pentahoBaseURL&quot;]&amp;&quot;plugin/cgg/api/services/draw?script=/public/SuiteCRM+Analytics/System/Resources/RCD/&quot; &amp; [fChartGeneratedURL] &amp; &quot;.js&amp;paramchart=AgentOpportunityPerformance&amp;paramchartType=ccc&quot; &amp; [pSelectChartType] &amp; &quot;Chart&quot; &amp; [fChartParams]"/>
  <expression name="FUNC_TOTAL_OPEN" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">OPEN_OPPORTUNITY_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_CLOSED_WON" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CLOSED_WON_OPPORTUNITY_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_CLOSED_LOST" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CLOSED_LOST_OPPORTUNITY_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_OPPORTUNITIES" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">TOTAL_OPPORTUNITIES</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_OPEN_VALUE" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">OPEN_OPPORTUNITY_AMOUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_CLOSED_WON_VALUE" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CLOSED_WON_OPPORTUNITY_AMOUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_CLOSED_LOST_VALUE" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CLOSED_LOST_OPPORTUNITY_AMOUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_OPPORTUNITIES_VALUE" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">TOTAL_OPPORTUNITY_AMOUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_PAGE_NUMBER" class="org.pentaho.reporting.engine.classic.core.function.PageFunction">
    <properties>
      <property name="pageIncrement">1</property>
      <property name="startPage">1</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_ROWS" class="org.pentaho.reporting.engine.classic.core.function.TotalItemCountFunction">
    <properties>
    </properties>
  </expression>
  <expression name="fChartGeneratedURL" formula="=&quot;CGGChartGenerator&quot;"/>
</data-definition>
