<?xml version="1.0" encoding="UTF-8"?>
<data-definition xmlns="http://reporting.pentaho.org/namespaces/engine/classic/bundle/data/1.0">
  <parameter-definition>
    <list-parameter name="pSelectStyle" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pStyles" key-column="Value" value-column="Name" default-value="SuiteCRMAnalytics">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Select Report Style</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pCampaignFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pCampaignFilterQuery" key-column="CAMPAIGN_NAME" value-column="CAMPAIGN_NAME">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Campaign</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pCampaignStatusFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pCampaignStatusFilterQuery" key-column="CAMPAIGN_STATUS" value-column="CAMPAIGN_STATUS">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Campaign Status</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pCampaignTypeFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pCampaignTypeFilterQuery" key-column="CAMPAIGN_TYPE" value-column="CAMPAIGN_TYPE">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Campaign Type</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowChart" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pShowChartQuery" key-column="value" value-column="property" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Visualizations</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pSelectChartType" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pChartTypeQuery" key-column="ID" value-column="Value">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Chart Type</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">true</attribute>
    </list-parameter>
    <list-parameter name="pShowParameters" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pShowParameters" key-column="Value" value-column="property" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Parameters</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowDeletedRecords" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.Number" query="ShowDeletedRecords" key-column="Value" value-column="Name" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Deleted Records</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
  </parameter-definition>
  <data-source report-query="Report" limit="-1" timeout="0" ref="datasources/compound-ds.xml"/>
  <expression name="FUNC_PAGE_NUMBERS" class="org.pentaho.reporting.engine.classic.core.function.PageOfPagesFunction">
    <properties>
      <property name="format">page {0} of {1}</property>
      <property name="pageIncrement">1</property>
      <property name="startPage">1</property>
    </properties>
  </expression>
  <expression name="FUNC_TODAYS_DATE" formula="=TODAY()"/>
  <expression name="FUNC_AVG_SPEND" formula="=[CLOSED_WON_OPPORTUNITY_AMOUNT]/[CLOSED_WON_OPPORTUNITY_COUNT]"/>
  <expression name="FUNC_ROI" formula="=(([CLOSED_WON_OPPORTUNITY_COUNT]*[FUNC_AVG_SPEND])-[CAMPAIGN_ACTUAL_COST])/[CAMPAIGN_ACTUAL_COST]"/>
  <expression name="fChartURL" formula="=[&quot;env::pentahoBaseURL&quot;]&amp;&quot;plugin/cgg/api/services/draw?script=/public/SuiteCRM+Analytics/System/Resources/RCD/&quot; &amp; [fChartGeneratedURL] &amp; &quot;.js&amp;paramchart=CampaignPerformance&amp;paramchartType=ccc&quot; &amp; [pSelectChartType] &amp; &quot;Chart&quot; &amp; [fChartParams]"/>
  <expression name="fChartParams" formula="=&quot;&amp;parampCampaignTypeFilter=&quot;&amp; URLENCODE([pCampaignTypeFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampCampaignStatusFilter=&quot;&amp; URLENCODE([pCampaignStatusFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampCampaignFilter=&quot;&amp; URLENCODE([pCampaignFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampShowDeletedRecords=&quot;&amp; URLENCODE([pShowDeletedRecords];&quot;UTF8&quot;) &amp;&quot;&amp;outputType=svg&amp;_TRUST_USER_=&quot;&amp;[&quot;env::username&quot;]"/>
  <expression name="FUNC_PAGE_NUMBER" class="org.pentaho.reporting.engine.classic.core.function.PageFunction">
    <properties>
      <property name="pageIncrement">1</property>
      <property name="startPage">1</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_ROWS" class="org.pentaho.reporting.engine.classic.core.function.TotalItemCountFunction">
    <properties>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_OPEN_COUNT" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">OPEN_OPPORTUNITY_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_LOST_COUNT" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CLOSED_LOST_OPPORTUNITY_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_WON_COUNT" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CLOSED_WON_OPPORTUNITY_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_BUDGET" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CAMPAIGN_BUDGET</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_ACTUAL_COST" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CAMPAIGN_ACTUAL_COST</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_REVENUE" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CLOSED_WON_OPPORTUNITY_AMOUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_AVG_SPEND" class="org.pentaho.reporting.engine.classic.core.function.ItemAvgFunction">
    <properties>
      <property name="field">FUNC_AVG_SPEND</property>
      <property name="roundingMode">4</property>
      <property name="scale">14</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_ROI" class="org.pentaho.reporting.engine.classic.core.function.ItemAvgFunction">
    <properties>
      <property name="field">FUNC_ROI</property>
      <property name="roundingMode">4</property>
      <property name="scale">14</property>
    </properties>
  </expression>
  <expression name="fChartGeneratedURL" formula="=&quot;CGGChartGenerator&quot;"/>
</data-definition>
