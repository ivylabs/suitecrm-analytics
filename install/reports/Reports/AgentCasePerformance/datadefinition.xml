<?xml version="1.0" encoding="UTF-8"?>
<data-definition xmlns="http://reporting.pentaho.org/namespaces/engine/classic/bundle/data/1.0">
  <parameter-definition>
    <list-parameter name="pSelectStyle" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pStyles" key-column="Value" value-column="Name" default-value="SuiteCRMAnalytics">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Select Report Style</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pCaseCreatedDateFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pCaseCreatedDateFilterQuery" key-column="YYYY-MM" value-column="DATE_FILTER">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Case Created Period</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pCaseClosedDateFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pCaseClosedDateFilterQuery" key-column="YYYY-MM" value-column="DATE_FILTER">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Case Closed Period</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pCaseTypeFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pCaseTypeFilterQuery" key-column="CASE_TYPE" value-column="CASE_TYPE">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Case Type</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pCasePriorityFilter" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pCasePriorityFilterQuery" key-column="CASE_PRIORITY" value-column="CASE_PRIORITY">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Case Priority</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowChart" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pShowChartQuery" key-column="value" value-column="property" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Visualizations</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pSelectChartType" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pChartTypeQuery" key-column="ID" value-column="Value">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Chart Type</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowParameters" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.String" query="pShowParameters" key-column="Value" value-column="property" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Parameters</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
    <list-parameter name="pShowDeletedRecords" allow-multi-selection="false" strict-values="true" mandatory="false" type="java.lang.Number" query="ShowDeletedRecords" key-column="Value" value-column="Name" default-value="1">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="parameter-render-type">dropdown</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="re-evaluate-on-failed-values">false</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="autofill-selection">true</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Deleted Records</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </list-parameter>
  </parameter-definition>
  <data-source report-query="Report" limit="-1" timeout="0" ref="datasources/compound-ds.xml"/>
  <expression name="FUNC_PAGE_NUMBERS" class="org.pentaho.reporting.engine.classic.core.function.PageOfPagesFunction">
    <properties>
      <property name="format">page {0} of {1}</property>
      <property name="pageIncrement">1</property>
      <property name="startPage">1</property>
    </properties>
  </expression>
  <expression name="FUNC_TODAYS_DATE" formula="=TODAY()"/>
  <expression name="fChartGeneratedURL" formula="=&quot;ReportChartDefinitionGeneratedURL&quot;"/>
  <expression name="fChartParams" formula="=&quot;&amp;parampCaseTypeFilter=&quot;&amp; URLENCODE([pCaseTypeFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampCasePriorityFilter=&quot;&amp; URLENCODE([pCasePriorityFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampDateFilter=&quot;&amp; URLENCODE([pCaseCreatedDateFilter];&quot;UTF8&quot;) &amp;&quot;&amp;parampShowDeletedRecords=&quot;&amp; URLENCODE([pShowDeletedRecords];&quot;UTF8&quot;) &amp;&quot;&amp;outputType=svg&amp;_TRUST_USER_=&quot;&amp;[&quot;env::username&quot;]"/>
  <expression name="fChartURL" formula="=[&quot;env::pentahoBaseURL&quot;]&amp;&quot;plugin/cgg/api/services/draw?script=/public/SuiteCRM+Analytics/System/Resources/RCD/AgentCasePerformance/&quot;&amp; [fChartGeneratedURL] &amp; &quot;-&quot;&amp; [pSelectChartType] &amp;&quot;_COUNT_CASE_STATUS.js&quot;&amp;[fChartParams]"/>
  <expression name="fReportOutputType" formula="=IF(ISEXPORTTYPE(&quot;table/excel&quot;);&quot;This is Excel!&quot;;&quot;This is NOT Excel&quot;)"/>
  <expression name="FUNC_TOTAL_NEW" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">NEW_CASE_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_ASSIGNED" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">ASSIGNED_CASE_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_PENDING" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">PENDING_INPUT_CASE_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_CLOSED" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">CLOSED_CASE_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_DUPLICATE" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">DUPLICATE_CASE_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_REJECTED" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">REJECTED_CASE_COUNT</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_CASES" class="org.pentaho.reporting.engine.classic.core.function.TotalGroupSumFunction">
    <properties>
      <property name="field">TOTAL_CASES</property>
    </properties>
  </expression>
  <expression name="FUNC_PAGE_NUMBER" class="org.pentaho.reporting.engine.classic.core.function.PageFunction">
    <properties>
      <property name="pageIncrement">1</property>
      <property name="startPage">1</property>
    </properties>
  </expression>
  <expression name="FUNC_TOTAL_ROWS" class="org.pentaho.reporting.engine.classic.core.function.TotalItemCountFunction">
    <properties>
    </properties>
  </expression>
</data-definition>
